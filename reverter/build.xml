<?xml version="1.0" encoding="utf-8"?>
<project name="reverter" default="dist" basedir=".">
    <!-- enter the SVN commit message -->
    <property name="commit.message" value="Reverter: fix deleted/redacted relation members"/>
    <!-- enter the *lowest* JOSM version this plugin is currently compatible with -->
    <property name="plugin.main.version" value="18173"/>

    <!-- Configure these properties (replace "..." accordingly).
         See https://josm.openstreetmap.de/wiki/DevelopersGuide/DevelopingPlugins
    -->
    <property name="plugin.author" value="Upliner"/>
    <property name="plugin.class" value="reverter.ReverterPlugin"/>
    <property name="plugin.description" value="Plugin for reverting changesets"/>
    <property name="plugin.icon" value="images/revert-changeset.svg"/>
    <property name="plugin.link" value="https://wiki.openstreetmap.org/wiki/JOSM/Plugins/Reverter"/>
    <property name="plugin.canloadatruntime" value="true"/>
    <property name="plugin.stage" value="5"/>

    <!-- ** include targets that all plugins have in common ** -->
    <import file="../build-common.xml"/>
</project>

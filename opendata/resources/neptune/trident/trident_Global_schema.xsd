	<xsd:simpleType name="PTDirectionType">
		<xsd:annotation>
			<xsd:documentation>
		Enumeration containing all the possible directions on a PT Network
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="North"/>
